{
    "ignore_patterns": [
        ".*",
        "__pycache__",
        "*.pyc",
        "*.pyo",
        "*.so",
        "*.dll",
        "*.o",
        "*.a",
        "*.dylib",
        "*.egg-info",
        "build/",
        "dist/",
        "node_modules/",
        "vendor/",
        "target/",
        "*.log",
        "*.swp",
        "*.swo",
        "CONTENT.xml",
        "CONTENT.md",
        "TREE.xml",
        "TREE.md",
        "SUMMARY.xml",
        "SUMMARY.md"
    ],
    "text_extensions": [
        ".py",
        ".js",
        ".jsx",
        ".ts",
        ".tsx",
        ".java",
        ".c",
        ".cpp",
        ".h",
        ".hpp",
        ".cs",
        ".go",
        ".rs",
        ".php",
        ".rb",
        ".swift",
        ".kt",
        ".kts",
        ".scala",
        ".pl",
        ".pm",
        ".sh",
        ".bash",
        ".zsh",
        ".ps1",
        ".lua",
        ".groovy",
        ".dart",
        ".html",
        ".htm",
        ".css",
        ".scss",
        ".sass",
        ".less",
        ".vue",
        ".svelte",
        ".xml",
        ".json",
        ".yaml",
        ".yml",
        ".toml",
        ".ini",
        ".cfg",
        ".conf",
        ".md",
        ".markdown",
        ".rst",
        ".txt",
        ".sql",
        ".graphql",
        ".gql",
        ".dockerfile",
        "Dockerfile",
        ".gitignore",
        ".env",
        ".properties",
        ".gradle",
        ".tf",
        ".tfvars",
        ".hcl"
    ],
    "max_content_size_mb": 5,
    "report_filenames": {
        "xml": {
            "summary": "SUMMARY.xml",
            "tree": "TREE.xml",
            "content": "CONTENT.xml"
        },
        "md": {
            "summary": "SUMMARY.md",
            "tree": "TREE.md",
            "content": "CONTENT.md"
        }
    },
    "default_format": "xml"
}